trait Enumerable<T> {
    def map(func: (T) -> R) -> Array<R> {
        let mut values: Array<R> = []

        each (value) {
            values.push(func(value))
        }

        values
    }
}

class Array<T = dynamic> {
    use Enumerable<T>, select = 'array_select', map = 'array_map'
}

